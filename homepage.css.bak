*{
	margin: 0;
	padding: 0;
	box-sizing: border-box;
}

body{
	background-color: #f77e21;
	color: #fff;
}

div.navbar{
	background-color: #d61c4e;
	width: 100%;
	height: 60px;
}

ul.navbar{
	list-style: none;
	display: flex;
	align-items: center;
	justify-items: center;
	margin: 2px;
	text-align: center;
	float: right;
}

li.navbar{
		font-family: 'Georama';
		width: 6em;
		height: 50px;
		font-size: 20px;
		/*margin-top: 12px; */
}
